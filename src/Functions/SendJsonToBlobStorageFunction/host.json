{
  "extensions": {
    "http": {
      "cors": {
        "allowedOrigins": [ "*" ],
        "allowedMethods": [ "GET", "POST" ],
        "supportCredentials": false
      }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "version": "2.0"
}